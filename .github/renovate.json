{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "addLabels": [
    "ok-to-test"
  ],
  "baseBranches": [
    "main",
    "release-2.11",
    "release-2.12",
    "release-2.13",
    "release-2.14"
  ],
  "gomod": {
    "packageRules": [
      {
        "matchBaseBranches": [
          "release-2.11",
          "release-2.12",
          "release-2.13",
          "release-2.14"
        ],
        "matchManagers": [
          "gomod"
        ],
        "enabled": false
      }
    ]
  },
  "vulnerabilityAlerts": {
    "enabled": true
  },
  "osvVulnerabilityAlerts": true,
  "rebaseWhen": "behind-base-branch",
  "prConcurrentLimit": 0,
  "timezone": "America/New_York",
  "dependencyDashboard": true
}
